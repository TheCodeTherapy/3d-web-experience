{
  "name": "@apps/3d-web-experience-server-advanced",
  "private": true,
  "version": "0.22.0",
  "engines": {
    "node": ">=20.x"
  },
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:react-mml && rimraf ./build && tsx ./build.ts --build",
    "build:react-mml": "cd react-mml-documents && tsx build.ts --build",
    "iterate": "tsx ./build.ts --watch",
    "iterate:react-mml": "cd react-mml-documents && tsx build.ts --watch",
    "iterate:start": "node ./build/index.js",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"./{src,test}/**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint-fix": "eslint \"./{src,test}/**/*.{js,jsx,ts,tsx}\" --fix"
  },
  "dependencies": {
    "@dimforge/rapier3d": "^0.13.0",
    "@dolbyio/dolbyio-rest-apis-client": "4.1.0",
    "@apps/3d-web-experience-client": "^0.22.0",
    "@mml-io/3d-web-experience-server": "^0.22.0",
    "@mml-io/3d-web-text-chat": "^0.22.0",
    "@mml-io/3d-web-user-networking": "^0.22.0",
    "@mml-io/mml-react-types": "0.19.3",
    "@mml-io/networked-dom-server": "0.21.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "dotenv": "16.4.5",
    "express": "4.21.2",
    "express-ws": "5.0.2",
    "livekit-server-sdk": "2.6.0",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/node-fetch": "^2.6.12",
    "@types/express": "^5.0.0",
    "@types/express-ws": "^3.0.5",
    "@types/node": "^22.13.1",
    "esbuild": "^0.24.0",
    "esbuild-plugin-wasm": "^1.1.0"
  }
}
